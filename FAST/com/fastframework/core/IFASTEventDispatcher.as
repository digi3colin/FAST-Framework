/** * @author colin */package com.fastframework.core{	import flash.events.IEventDispatcher;	public interface IFASTEventDispatcher extends IEventDispatcher{		//weak reference to the callback		function when(eventType:String,whichObject:Object,callback:Function):*;		//only listen to the event one time		function once(eventType:String,whichObject:Object,callback:Function):*;	}}